<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:minHeight="80dp">

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/startdate"
        android:text="Start Date"
        android:gravity="center"
        android:background="@drawable/input_background"
        android:layout_marginStart="55dp"
        android:textSize="18sp"
        android:layout_marginTop="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/enddate"
        android:text="End Date"
        android:gravity="center"
        android:background="@drawable/input_background"
        android:textSize="18sp"
        android:layout_marginEnd="66dp"
        android:layout_alignBaseline="@+id/startdate"
        android:layout_alignBottom="@+id/startdate"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/Single"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#E3F2FD"
        android:textSize="18sp"
        android:layout_marginTop="44dp"
        android:text="Single Room"
        android:layout_below="@+id/startdate"
        android:layout_alignStart="@+id/startdate" />

    <TextView
        android:id="@+id/Double"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Double Room"
        android:textSize="18sp"
        android:textColor="#E3F2FD"
        android:layout_below="@+id/Single"
        android:layout_alignStart="@+id/Single"
        android:layout_marginTop="29dp" />

    <TextView
        android:id="@+id/Deluxe"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#E3F2FD"
        android:text="Deluxe Room"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/Double"
        android:layout_alignStart="@+id/Double" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/singleprice"
        android:hint="Enter Price"
        android:gravity="center"
        android:focusableInTouchMode="true"
        android:background="@drawable/input_background"
        android:layout_alignTop="@+id/Deluxe"
        android:layout_alignStart="@+id/doubleprice" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Enter Price"
        android:gravity="center"
        android:focusableInTouchMode="true"
        android:background="@drawable/input_background"
        android:id="@+id/doubleprice"
        android:layout_alignTop="@+id/Double"
        android:layout_alignStart="@+id/deluxeprice" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Enter Price"
        android:gravity="center"
        android:focusableInTouchMode="true"
        android:background="@drawable/input_background"
        android:id="@+id/deluxeprice"
        android:layout_alignBottom="@+id/Single"
        android:layout_alignStart="@+id/enddate" />

    <Button
        android:id="@+id/price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Set Price"
        android:layout_below="@+id/Deluxe"
        android:layout_centerHorizontal="true" />

